[duo]
; Duo integration key
ikey=<%= scope.lookupvar('duo_unix::ikey') %>

; Duo secret key
skey=<%= scope.lookupvar('duo_unix::skey') %>

; Duo API host
host=<%= scope.lookupvar('duo_unix::host') %>

; Fallback local IP
fallback_local_ip=<%= scope.lookupvar('duo_unix::fallback_local_ip') %>

; Failure mode
failmode=<%= scope.lookupvar('duo_unix::failmode') %>

; Push info
pushinfo=<%= scope.lookupvar('duo_unix::pushinfo') %>

; Auto push
autopush=<%= scope.lookupvar('duo_unix::autopush') %>

; Prompts
prompts=<%= scope.lookupvar('duo_unix::prompts') %>

; Accept environment factor
accept_env_factor=<%= scope.lookupvar('duo_unix::accept_env_factor') %>
<% if scope.lookupvar('duo_unix::usage') == 'login' -%>

; MOTD display
motd=<%= scope.lookupvar('duo_unix::motd') %>
<% end -%>
<% if scope.lookupvar('duo_unix::group') != '' -%>

; Group restriction
group=<%= scope.lookupvar('duo_unix::group') %>
<% end -%>
<% if scope.lookupvar('duo_unix::http_proxy') != '' -%>

; HTTP proxy
http_proxy=<%= scope.lookupvar('duo_unix::http_proxy') %>
<% end -%>
